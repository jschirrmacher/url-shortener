{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "URL-Shortener mit Analytics und CSV-Speicher (Nuxt3 TypeScript)",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.12.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "jsonwebtoken": "9.0.2",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@prettier/plugin-xml": "3.4.2",
    "@types/jsonwebtoken": "9.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "nuxt": "^3.13.2",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,md}": [
      "prettier --write"
    ]
  }
}
